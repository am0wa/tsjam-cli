{
  "name": "tsjam-cli",
  "version": "0.0.3",
  "description": "CLI for ts react webpack & friends",
  "main": "",
  "keywords": [
    "atomic webpack ts react configs"
  ],
  "author": "am0wa",
  "contributors": [
    "am0wa"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/am0wa/tsjam/issues"
  },
  "homepage": "https://github.com/am0wa/tsjam#readme",
  "scripts": {
    "build": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky install",
    "postinstall": "node bin/cli.js --help"
  },
  "files": [
    "bin",
    "scripts",
    "react-template",
    "tsjam-template",
    "webpack",
    "package.json"
  ],
  "bin": {
    "tsjam-cli": "bin/cli.js"
  },
  "peerDependencies": {
    "typescript": ">=4",
    "webpack": "5.x",
    "webpack-cli": "5.x"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "8.0.1",
    "@swc/core": "^1.3.72",
    "@tsjam/eslint-config-recommended": "^0.1.0",
    "@tsjam/web-config-base": "^0.1.1",
    "@types/postcss-pxtorem": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "eslint-webpack-plugin": "^4.0.1",
    "favicons-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.9.0"
  },
  "prettier": "@tsjam/web-config-base/prettier.config.js",
  "lint-staged": {
    "*.{ts,tsx,js}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  }
}
