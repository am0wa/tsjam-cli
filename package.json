{
  "name": "@tsjam/webpack-parts",
  "version": "0.0.1",
  "description": "Shared webpack plugins configurations: webpack",
  "main": "",
  "keywords": [
    "shared webpack config"
  ],
  "author": "am0wa",
  "contributors": [
    "am0wa"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/am0wa/tsjam/issues"
  },
  "homepage": "https://github.com/am0wa/tsjam#readme",
  "scripts": {
    "wp:start": "webpack serve --node-env=development --config .webpack/config.development.ts",
    "wp:build": "webpack build --node-env=production --config .webpack/config.production.ts",
    "build": "tsc --project ./tsconfig.json",
    "clean:dist": "rimraf lib",
    "code:format": "prettier './**/**.{js,jsx,ts,tsx}' --write",
    "prepublishOnly": "run-s clean:dist build",
    "prepare": "husky install",
    "postinstall": "node cli/eject.js"
  },
  "files": [
    "cli",
    "scripts",
    "webpack",
    "package.json"
  ],
  "bin": {
    "wp-eject": "cli/eject.js"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "8.0.1",
    "@swc/core": "^1.3.72",
    "@types/postcss-pxtorem": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "eslint-webpack-plugin": "^4.0.1",
    "favicons-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "webpack": "5.x",
    "webpack-cli": ">=5.x",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.9.0"
  },
  "peerDependencies": {
    "typescript": ">=4",
    "webpack": "5.x",
    "webpack-cli": ">=5.x"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,scss,md}": "prettier --write"
  }
}
