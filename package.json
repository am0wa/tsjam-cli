{
  "name": "tsjam-cli",
  "version": "0.1.7",
  "description": "CLI for ts react webpack & friends",
  "main": "",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "Atomic ts esm, react, jest, webpack configs"
  ],
  "author": "am0wa",
  "contributors": [
    "am0wa"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/am0wa/tsjam/issues"
  },
  "homepage": "https://github.com/am0wa/tsjam#readme",
  "scripts": {
    "build": "tsc --project ./tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "prepare": "husky",
    "postinstall": "node bin/cli.mjs --help",
    "format": "prettier './**/**.{mjs,js,jsx,ts,tsx,css,scss}' --write"
  },
  "files": [
    "bin",
    "scripts",
    "react-template",
    "jest-template",
    "tsjam-template",
    "tsjam-template/.npmrc",
    "tsjam-template/gitignore",
    "webpack",
    "package.json"
  ],
  "bin": {
    "tsjam-cli": "bin/cli.mjs"
  },
  "peerDependencies": {
    "typescript": ">=5",
    "webpack": "5.x",
    "webpack-cli": "5.x"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.13.5",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsjam/eslint-config-recommended": "^0.2.3",
    "@tsjam/jest-config-recommended": "^0.4.0",
    "@tsjam/web-config-base": "^0.3.1",
    "@types/postcss-pxtorem": "^6.1.0",
    "autoprefixer": "^10.4.21",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-modules-typescript-loader": "^4.0.1",
    "cssnano": "^7.1.1",
    "dotenv": "^17.2.1",
    "eslint-webpack-plugin": "^5.0.2",
    "favicons-webpack-plugin": "^6.0.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.1.1",
    "postcss-pxtorem": "^6.1.0",
    "react-dev-utils": "^12.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.91.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-manifest-plugin": "^5.0.1",
    "webpack-merge": "^6.0.1"
  },
  "optionalDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "prettier": "@tsjam/web-config-base/prettier.config.mjs",
  "lint-staged": {
    "*.{ts,tsx,js}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  },
  "dependencies": {
    "rxjs": "^7.8.2",
    "tsjam": "^1.7.2",
    "yoctocolors": "^2.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
